{
  "name": "budget-buddy-server",
  "version": "0.0.1",
  "description": "Budgeting and financing application",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:production": "heroku run npm run migrate",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master",
    "postdeploy": "npm run migrate:production"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.13",
    "morgan": "^1.10.0",
    "pg": "^7.8.0",
    "postgrator-cli": "^3.2.0",
    "treeize": "^2.1.2",
    "xss": "^1.0.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3",
    "supertest": "^3.3.0"
  },
  "contributors": [
    "Johnny Juarez, johnnyjuarez1992@gmail.com"
  ],
  "license": "MIT"
}
